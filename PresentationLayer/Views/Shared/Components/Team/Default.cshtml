@model List<DataAccessLayer.Entities.Team>
<section id="team" class="team ">
    <div class="container">
        <div class="section-title">
            <h2>Takım Arkadaşlarımız</h2>
        </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {


                <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
                    <div class="member">
                        <img src="~/Img/@item.Image" alt="">
                        <h4>@item.FullName</h4>
                        <span>@item.Duty</span>

                        <div class="social">
                            <a href="@item.Twitter"><i class="bi bi-twitter"></i></a>
                            <a href="@item.Facebook"><i class="bi bi-facebook"></i></a>
                            <a href="@item.Instegram"><i class="bi bi-instagram"></i></a>
                            <a href="@item.LinkedIn"><i class="bi bi-linkedin"></i></a>
                        </div>
                    </div>

                </div>
            }
        </div>
</section><!-- End Team Section -->